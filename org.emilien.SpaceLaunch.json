{
    "app-id" : "org.emilien.SpaceLaunch",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "42",
    "sdk" : "org.gnome.Sdk",
    "command" : "spacelaunch",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland"
