        "/share/vala",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "spacelaunch",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "https://gitlab.com/elescoute/spacelaunch",
                    "commit": "aeaa4e570c728a141e5e64a4e7da1d8477378d02"
                }
            ]
        }
    ]
